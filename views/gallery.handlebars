<head>
    <link rel="stylesheet" href="/css/gallery.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Raleway&family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap"
        rel="stylesheet">
</head>

<body>
    <main>
        <h1>Our Fleet</h1>
        <div>
            <div class="float-container">
                {{#each cars}}
                <div class="fleet-container">
                    <img src="/images/{{image}}.jpg">
                    <h2>{{title}}</h2>
                    <h3>{{description}}</h3>
                    <h3>{{price}}</h3>
                    <button>Book Now</button>
                    {{!-- <button><a href="/rent/{{carId}}">Book Now</a></button> --}}
                </div>
                {{/each}}


            </div>
        </div>
    </main>

</body>