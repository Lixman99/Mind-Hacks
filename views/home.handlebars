<div id="carouselExample" class="carousel slide w-75">
    <div class="carousel-inner w-75">
        {{#each cars as | car index | }}
        <div class="carousel-item{{#unless index}} active {{/unless}}">
            <div id="flex">
                <H2 id="title">Select your classic vintage vehicle</h2>
                <a id="a-tag" href="/rent/{{carId}}">Book Now</a>
            </div>
            <img src="/images/{{image}}.jpg" class="d-block w-100">
        </div>
        {{/each}}
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
        <span class="button-style carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden ">Previous</span>
    </button>
    <button class=" carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
        <span class="button-style carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>
<div class="welcome-message-wrapper">
    <div id="welcome-message"></div>
</div>
</div>
<script src="js/home.js"></script>